<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		version="3.1">
		
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<description>
  	</description>
  	<display-name>HelloServlet</display-name>
  	<servlet-name>HelloServlet</servlet-name>
  	<servlet-class>com.kudangtang.jv300.mod002.HelloServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<description>
  	</description>
  	<display-name>LifecycleTestServlet</display-name>
  	<servlet-name>LifecycleTestServlet</servlet-name>
  	<servlet-class>com.kudangtang.jv300.mod003.LifecycleTestServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<description>
  	</description>
  	<display-name>ServletConfigTestServlet</display-name>
  	<servlet-name>ServletConfigTestServlet</servlet-name>
  	<servlet-class>com.kudangtang.jv300.mod003.ServletConfigTestServlet</servlet-class>
  	<init-param>
  		<param-name>season-list</param-name>
  		<param-value>Spring,Summer,Fall,Winter</param-value>
  	</init-param>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>FormBasedHelloServlet</display-name>
  	<servlet-name>FormBasedHelloServlet</servlet-name>
  	<servlet-class>com.kudangtang.jv300.mod004.FormBasedHelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>AddUserServlet</display-name>
  	<servlet-name>AddUserServlet</servlet-name>
  	<servlet-class>com.kudangtang.jv300.mod004.AddUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/Hello.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LifecycleTestServlet</servlet-name>
  	<url-pattern>/lifecycle.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletConfigTestServlet</servlet-name>
  	<url-pattern>/config.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FormBasedHelloServlet</servlet-name>
  	<url-pattern>/mod004/form.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddUserServlet</servlet-name>
  	<url-pattern>/AddUserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddUserServlet</servlet-name>
  	<url-pattern>/mod004/add_user.view</url-pattern>
  </servlet-mapping>
</web-app>